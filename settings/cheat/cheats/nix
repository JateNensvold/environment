# Search package 
nix-env --query --available --attr-path

# Load shell.nix or default.nix
nix-shell

# Install Nix package to profile
nix profile install nixpkgs#git-crypt

# Build derive
nix build .#<derive name>

# List packages
nix-env -qaP 
# List home-manager packages
home-manager packages

#Clear cache
nix-collect-garbage -d
